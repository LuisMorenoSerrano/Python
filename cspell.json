{
  "version": "0.2",
  "language": "en",
  "enabledLanguageIds": [
    "python"
  ],
  // Custom dictionary words
  "words": [
    "ALPH",
    "arcoscubicos",
    "Astar",
    "autoscale",
    "axisymmetric",
    "bgrcmy",
    "BLAS",
    "BMACH",
    "CONTUR",
    "conturpy",
    "cycol",
    "dashdot",
    "dgemm",
    "ETAD",
    "facecolor",
    "figheight",
    "figwidth",
    "findent",
    "FMACH",
    "fortls",
    "getchlib",
    "getcwd",
    "getkey",
    "gfortran",
    "itertools",
    "LAPACK",
    "libblas",
    "liblapack",
    "libopenblas",
    "linestyle",
    "lopenblas",
    "lpthw",
    "matplotlib",
    "meshgrid",
    "Mult",
    "NODO",
    "NOUP",
    "NPCT",
    "numpy",
    "pyplot",
    "seaborn",
    "Sivells",
    "streamwise",
    "tobera",
    "viridis",
    "VISM",
    "whitesmoke",
    "XEND",
    "XINC",
    "xlim",
    "XLOW",
    "XMID",
    "ylim",
    "Zavalan",
    "zorder"
  ],
  // Ignore patterns
  "ignoreRegExpList": [
    "/^\\s*import\\s+.*/gm",
    "/^\\s*from\\s+.*/gm",
    "/'[^']*'/g",
    "/\"[^\"]*\"/g"
  ],
  // Language-specific settings for Python
  "languageSettings": [
    {
      "languageId": "python",
      "includeRegExpList": [
        "/#.*/g",
        "/'''[\\s\\S]*'''/g",
        "/\"\"\"[\\s\\S]*\"\"\"/g"
      ]
    }
  ],
  // Ignore paths
  "ignorePaths": [
    "**/*.pyc",
    "**/__pycache__/**",
    "**/node_modules/**",
    "**/.git/**",
    "**/*.min.*"
  ]
}
